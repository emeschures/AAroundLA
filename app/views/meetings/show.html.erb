<!-- <h4>Thank you for your contribution!</h4> -->

<h1><%= @meeting.name %></h1>
<h2><%= @meeting.address %></h2>

<%# check to see if meeting was created by the current user. if so, show an edit button %>
<% if @meeting.user == current_user %>
    <%= link_to "Edit Meeting", edit_meeting_path(@meeting.id) %>
    ||
    <%= link_to "Delete Meeting", meeting_path(@meeting.id), {method: 'delete', data: {confirm: "Are you sure?"}} %>  
<% end %>